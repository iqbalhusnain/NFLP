<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.attempt2">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="NFLP"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@drawable/app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="ExtraText"
        tools:targetApi="q">
        <activity
            android:name=".generate_sheet"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".participants_area_third"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".participants_area_second"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".thankyou" />
        <activity
            android:name=".participants_area"
            android:label="Enter Participants Detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DataEntryPage"
            android:label="@string/title_activity_data_entry__page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>
